{
    "name": "magedawg/magento2-custom-maintenance",
    "description": "MageDawg Custom Maintenance for Magento 2",
    "type": "magento2-component",
    "authors": [
        {
            "name": "MageDawg",
            "email": "magedawg@gmail.com"
        }
    ],
    "require": {
        "php": "~5.5.0|>=5.6.0",
        "magento/magento-composer-installer": "*"        
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "MageDawg\\CustomMaintenance\\": ""
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "map": [
            [
                "pub/errors/magedawg_maintenance",
                "pub/errors/magedawg_maintenance"
            ]
        ]
    }
}
